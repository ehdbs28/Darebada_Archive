                        <ui:Label text="7월14일 아쿠아리움 명세서" display-tooltip-when-elided="true" name="title-text" class="text" style="font-size: 40px;" />
                    </ui:VisualElement>
                    <ui:VisualElement name="footer-content" style="flex-basis: 40%; flex-direction: row; align-items: center; justify-content: center; flex-shrink: 0;">
                        <ui:VisualElement name="profile-box" style="width: 50%; height: 40px; left: 10px; position: absolute; overflow: hidden; flex-direction: row; align-items: center;">
                            <ui:Label text="아쿠아리움 관리인" display-tooltip-when-elided="true" name="name-text" class="text" style="font-size: 20px; -unity-font-style: bold;" />
                        </ui:VisualElement>
                        <ui:Label text="7월14일" display-tooltip-when-elided="true" name="date-text" class="text" style="position: absolute; right: 20px; font-size: 20px; letter-spacing: 7px;" />
                    </ui:VisualElement>
                    <ui:VisualElement name="desc-content">
                        <ui:Label text="입장료 수익&#10;부가구역 수익&#10;&#10;총 관리자 일급&#10;직원 일급&#10;아쿠아리움 관리비&#10;&#10;-----------------------------------------------------&#10;&#10;총 수익&#10;" display-tooltip-when-elided="true" name="desc-text" class="text" style="font-size: 25px; position: absolute; left: 15px; top: 15px; right: 15px; bottom: 15px; -unity-font-style: normal;" />
                        <ui:Label text="+100&#10;+56&#10;&#10;-10&#10;-5&#10;-30&#10;&#10;&#10;&#10;+111$" display-tooltip-when-elided="true" name="desc-price" class="text" style="position: absolute; left: 15px; top: 15px; right: 15px; bottom: 15px; font-size: 25px; -unity-text-align: upper-right;" />
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:VisualElement name="quest-letter" class="letter-unit">
                    <ui:VisualElement name="title-content" style="flex-basis: 60%; flex-direction: row; align-items: center;">
                        <ui:VisualElement name="type-image" style="width: 45px; height: 45px; left: 10px; background-image: url(&apos;project://database/Assets/05.Assets/UI/questicon.png?fileID=21300000&amp;guid=c3de7621bc9d2024198c14afb4c93f01&amp;type=3#questicon&apos;); margin-right: 20px;" />
                        <ui:Label text="아쿠아리움 건의사항 !!" display-tooltip-when-elided="true" name="title-text" class="text" style="font-size: 40px;" />
                    </ui:VisualElement>
                    <ui:VisualElement name="footer-content" style="flex-basis: 40%; flex-direction: row; align-items: center; justify-content: center;">
                        <ui:VisualElement name="profile-box" style="width: 50%; height: 40px; left: 10px; position: absolute; overflow: hidden; flex-direction: row; align-items: center;">
                            <ui:Label text="박부성" display-tooltip-when-elided="true" name="name-text" class="text" style="font-size: 20px; -unity-font-style: bold;" />
                        </ui:VisualElement>
