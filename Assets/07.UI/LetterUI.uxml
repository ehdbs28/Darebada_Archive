                                <ui:Label text="수락" display-tooltip-when-elided="true" name="text" class="text" style="-unity-text-align: middle-center; width: 100%; height: 100%; font-size: 30px;" />
                            </ui:VisualElement>
                            <ui:VisualElement name="no-btn" style="width: 200px; height: 50px; background-color: rgba(0, 0, 0, 0.29); border-top-left-radius: 5px; border-bottom-left-radius: 5px; border-top-right-radius: 5px; border-bottom-right-radius: 5px;">
                                <ui:Label text="거절" display-tooltip-when-elided="true" name="text" class="text" style="-unity-text-align: middle-center; width: 100%; height: 100%; font-size: 30px;" />
                            </ui:VisualElement>
