                        <ui:VisualElement name="type-image" style="width: 45px; height: 45px; left: 10px; background-image: url(&apos;project://database/Assets/05.Assets/UI/hearticon.png?fileID=-45266452&amp;guid=78cbb390cb63e754695d09a7f9118918&amp;type=3#hearticon_0&apos;); margin-right: 20px;" />
                        <ui:Label text="오늘 견학 후기" display-tooltip-when-elided="true" name="title-text" class="text" style="font-size: 40px;" />
                    </ui:VisualElement>
                    <ui:VisualElement name="footer-content" style="flex-basis: 40%; flex-direction: row; align-items: center; justify-content: center;">
                        <ui:VisualElement name="profile-box" style="width: 50%; height: 40px; left: 10px; position: absolute; overflow: hidden; flex-direction: row; align-items: center;">
                            <ui:Label text="이도윤" display-tooltip-when-elided="true" name="name-text" class="text" style="font-size: 20px; -unity-font-style: bold;" />
                        </ui:VisualElement>
