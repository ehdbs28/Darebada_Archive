.inventory-unit {
    width: 100px;
    height: 100px;
    background-color: rgba(255, 255, 255, 0.59);
    border-top-left-radius: 6px;
    border-bottom-left-radius: 6px;
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
    align-items: center;
    justify-content: center;
    margin-left: 1px;
    margin-right: 1px;
    margin-top: 1px;
    margin-bottom: 1px;
}

.fish-item {
    transform-origin: left top;
}
